{
  "name": "envfly-cli",
  "version": "1.0.0",
  "description": "Lightweight CLI for syncing environment variables across teams",
  "main": "src/index.js",
  "bin": {
    "envfly": "./bin/envfly.js"
  },
  "scripts": {
    "dev": "node bin/envfly.js",
    "test": "jest --passWithNoTests",
    "build": "echo \"Build step: nothing to compile\"",
    "pack": "npm pack",
    "prepublishOnly": "npm test && npm run pack",
    "publish-local": "npm pack && npm install -g envfly-cli-1.0.0.tgz",
    "release:patch": "npm version patch && git push && git push --tags",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:major": "npm version major && git push && git push --tags",
    "release:manual": "npm version",
    "clean": "rm -rf node_modules package-lock.json envfly-cli-*.tgz"
  },
  "keywords": [
    "cli",
    "environment",
    "variables",
    "sync",
    "team",
    "devops"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^4.1.2",
    "inquirer": "^8.2.6",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.2.0",
    "keytar": "^7.9.0",
    "listr2": "^8.0.0",
    "ora": "^5.4.1",
    "boxen": "^5.1.2"
  },
  "optionalDependencies": {
    "aws-sdk": "^2.1531.0",
    "@azure/identity": "^4.0.1",
    "@azure/keyvault-secrets": "^4.7.3",
    "@google-cloud/secret-manager": "^5.0.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^10.0.6",
    "semantic-release": "^22.0.7"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adarsh-technocrat/EnvFly-CLI.git"
  },
  "bugs": {
    "url": "https://github.com/adarsh-technocrat/EnvFly-CLI/issues"
  },
  "homepage": "https://github.com/adarsh-technocrat/EnvFly-CLI#readme",
  "files": [
    "bin/",
    "src/",
    "templates/",
    "README.md"
  ]
}
